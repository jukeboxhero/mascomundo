<div class="row" style="margin:0;background-color:#f9f9f9;">
    <div class="col-sm-6 sm-f-right" style="padding:0;">
        <div id="half-image-1" style="background: url('/assets/dogs.jpg') left center / cover no-repeat; width: 100%; min-height: 752px;">
            <%= image_tag("dogs.jpg", style: "width:100%;visibility:hidden;") %>
        </div>
    </div>
    <div id="half-content-1" class="col-sm-6" style="padding:4% 6% 1.5%;position:relative;text-align:left;">
        <h3 class="fullwidth-filter-title">Amazing New<br>Collection</h3>
        <p class="fullwidth-filter-description">Yes, this is our new collection, check it<br>out our new arrivals.</p>
        <div id="men_product" class="hide-addtocart hide-addtolinks no-border" style="margin-top: 25px;">
            <div class="products wrapper grid products-grid">
                <div class="filterproducts products list items product-items owl-carousel">

                    <% @latest_products.each do |product| %>
                      <%= render partial: "product", locals: { product: product } %>
                    <% end %>

                </div>
            </div>
        </div>
        <script type="text/javascript">
            $(function() {
                $("#men_product .owl-carousel").owlCarousel({
                    loop: true,
                    margin: 10,
                    autoplay: true,
                    autoplayTimeout: 5000,
                    autoplayHoverPause: true,
                    navRewind: true,
                    nav: false,
                    dots: true,
                    responsive: {
                        0: {
                            items:1
                        },
                        640: {
                            items:2
                        }
                    }
                });
            });
        </script>
    </div>
</div>
